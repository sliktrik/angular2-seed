{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "dist",
    "src/app/polyfills.ts"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": true
  }
}